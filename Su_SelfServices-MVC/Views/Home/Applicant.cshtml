    }

</style>




<script>
    window.onload = function () {
        $("#LblUserName").text("Applicant ID");
        $("#lnkSignup").removeClass("hidden");
        $("#lnkSignup").click(function () {
            window.URL = "~/Views/ForgotPassword";
        });
        $("#subApplicant").fadeIn(2000,200);
    }
</script>

